{
  "$schema": "../bucket-settings-schema.json",
  "get_ttl": "1h",
  "head_ttl": "30s",
  "compression_enabled": true,
  "prefix_overrides": [
    {
      "prefix": "/static/assets/",
      "get_ttl": "7d",
      "compression_enabled": true,
      "ram_cache_eligible": true
    },
    {
      "prefix": "/temp/",
      "get_ttl": "30s",
      "ram_cache_eligible": false
    },
    {
      "prefix": "/logs/",
      "read_cache_enabled": false
    },
    {
      "prefix": "/api-responses/",
      "get_ttl": "0s",
      "write_cache_enabled": true
    }
  ]
}
